/*Table structure for table `QUESTION_MASTER` */

DROP TABLE IF EXISTS `QUESTION_MASTER`;

CREATE TABLE `QUESTION_MASTER` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `NAME` varchar(500) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `QUESTION_KEYS` text CHARACTER SET utf8 COLLATE utf8_general_ci,
  `QUESTION` longtext,
  `ANSWER` longtext,
  `CREATED_TIME` timestamp NULL DEFAULT NULL,
  `SOFT_DELETE` int(11) DEFAULT '0',
  `QUESTION_FROM` int(11) DEFAULT NULL,
  `SUBJECT` int(11) NOT NULL DEFAULT '0',
  `BOOKMARK` int(11) DEFAULT '0',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB AUTO_INCREMENT=3562 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

