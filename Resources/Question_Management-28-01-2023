/*Table structure for table `MARKET_ANALYSER` */

DROP TABLE IF EXISTS `MARKET_ANALYSER`;

CREATE TABLE `MARKET_ANALYSER` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `SYMBOL` varchar(30) DEFAULT NULL,
  `DATE` varchar(30) DEFAULT NULL,
  `STOP_LOSS` int(11) DEFAULT NULL,
  `PROFIT` int(11) DEFAULT NULL,
  `IS_PROFIT` int(1) DEFAULT NULL COMMENT '1: Profit, 2: Loss',
  `TRADE_TYPE` int(1) DEFAULT NULL COMMENT '1:9.30-Strategy, 2:9.49-Strategy, 3:Reversal strategy',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB AUTO_INCREMENT=68 DEFAULT CHARSET=latin1;

