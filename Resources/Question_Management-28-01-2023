/*Table structure for table `TRADE_LOG_MASTER` */

DROP TABLE IF EXISTS `TRADE_LOG_MASTER`;

CREATE TABLE `TRADE_LOG_MASTER` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `TRADE_DATE` varchar(20) DEFAULT NULL,
  `DAY` varchar(100) DEFAULT NULL,
  `DATE_` int(11) DEFAULT NULL,
  `COMMENTS` text,
  `INDIA_VIX` varchar(10) DEFAULT NULL,
  `EVENTS` varchar(500) DEFAULT NULL,
  `CREATE_DATE` datetime DEFAULT NULL,
  `MODIFIED_DATE` datetime DEFAULT NULL,
  `SOFT_DELETE` int(11) DEFAULT '0',
  PRIMARY KEY (`ID`),
  UNIQUE KEY `UNIQUE_KEY_TRADE_DATE` (`TRADE_DATE`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=latin1;

