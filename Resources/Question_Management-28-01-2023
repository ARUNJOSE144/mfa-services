/*Table structure for table `TRADE_SYMBOLS` */

DROP TABLE IF EXISTS `TRADE_SYMBOLS`;

CREATE TABLE `TRADE_SYMBOLS` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `SYMBOL` varchar(100) DEFAULT NULL,
