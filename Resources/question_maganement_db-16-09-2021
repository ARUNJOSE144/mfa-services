DROP TABLE IF EXISTS `TOKEN_MASTER`;

CREATE TABLE `TOKEN_MASTER` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `TOKEN` varchar(1000) DEFAULT NULL,
