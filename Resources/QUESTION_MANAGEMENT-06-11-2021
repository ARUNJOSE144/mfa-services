(3518,'cpp program for BST(binart search tree) delete insert and traversal','cpp program for BST(binart search tree) delete insert and traversal','cpp program for BST(binart search tree) delete insert and traversal','#include <bits/stdc++.h>\n\nusing namespace std;\n\nstruct node {\n  int key;\n  struct node * left, * right;\n};\n\nstruct node * newNode(int item) // create a new BST node\n{\n  struct node * temp = (struct node * ) malloc(sizeof(struct node));\n  temp -> key = item;\n  temp -> left = temp -> right = NULL;\n  return temp;\n}\n\nvoid inorder(struct node * root) {\n  if (root != NULL) {\n    inorder(root -> left);\n    cout << root -> key << \" \";\n    inorder(root -> right);\n  }\n}\n\nstruct node * insert(struct node * node, int key) {\n  if (node == NULL) // If the tree is empty then return new node\n    return newNode(key);\n\n  if (key < node -> key) // else, recur down tree\n    node -> left = insert(node -> left, key);\n  else\n    node -> right = insert(node -> right, key);\n  return node;\n}\n\nstruct node * minValueNode(struct node * node) {\n  struct node * current = node;\n  while (current && current -> left != NULL) // loop to leftmost leaf\n    current = current -> left;\n  return current;\n}\n\nstruct node * deleteNode(struct node * root, int key) {\n\n  if (root == NULL) // base case\n    return root;\n\n  if (key < root -> key) //it lies in left subtree if (key < root->key) ,when the key to be deleted is smaller than the root\'s key\n    root -> left = deleteNode(root -> left, key);\n  else if (key > root -> key) // it lies in right subtree else if (key > root->key) ,when the key to be deleted is greater than the root\'s key\n    root -> right = deleteNode(root -> right, key);\n\n  // This is the node to be deleted ,when key is same as root\'s key\n  else {\n\n    if (root -> left == NULL and root -> right == NULL) // when node has no child\n      return NULL;\n    else if (root -> left == NULL) { // the node with no child or only one child\n      struct node * temp = root -> right;\n      free(root);\n      return temp;\n    } else if (root -> right == NULL) {\n      struct node * temp = root -> left;\n      free(root);\n      return temp;\n    }\n\n    struct node * temp = minValueNode(root -> right); // Get the inorder successor (smallest in the right subtree) when node with two children\n    root -> key = temp -> key; // Copy inorder successor\'s content to this node\n    root -> right = deleteNode(root -> right, temp -> key); // Delete inorder successor\n  }\n  return root;\n}\n\nint main() {\n  struct node * root = NULL;\n  root = insert(root, 10);\n  root = insert(root, 25);\n  root = insert(root, 20);\n  root = insert(root, 90);\n  root = insert(root, 45);\n  root = insert(root, 17);\n  root = insert(root, 12);\n  root = insert(root, 3);\n  root = insert(root, 5);\n\n  cout << \"**Inorder traversal of given tree**\\n\";\n  inorder(root);\n\n  cout << \"\\nDelete node 25\\n\";\n  root = deleteNode(root, 25);\n  cout << \"Inorder traversal of tree after deletion of 25\\n\";\n  inorder(root);\n\n  cout << \"\\nDelete node 90\\n\";\n  root = deleteNode(root, 90);\n  cout << \"Inorder traversal of tree after deletion of 90\\n\";\n  inorder(root);\n\n  return 0;\n}','2021-11-06 20:39:47',0,3,4,0);

